<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>

<addon id="plugin.program.zukzeuk"
  version="0.1.0"
  name="ZukZeuk WIZARD"
  provider-name="ZukZeuk">
  <requires>
    <import addon="xbmc.gui" version="5.14.0" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.repository" name="ZukZeuk">
	<info compressed="false">https://raw.githubusercontent.com/ezdakit/ZukZeuk_repo/main/addons.xml</info>
	<checksum>https://raw.githubusercontent.com/ezdakit/ZukZeuk_repo/main/addons.xml.md5</checksum>
    <datadir zip="true">https://raw.githubusercontent.com/ezdakit/ZukZeuk_repo/main</datadir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="es">Wizard para Kodi.</summary>
    <description lang="es">enjoy!!</description>
    <disclaimer lang="es"></disclaimer>
    <assets>
      <icon>icon.png</icon>
      <fanart>fanart.jpg</fanart>
      <screenshot>resources/screenshot000.jpg</screenshot>
      <screenshot>resources/screenshot001.jpg</screenshot>
      <screenshot>resources/screenshot002.jpg</screenshot>
    </assets>
  </extension>
</addon>

<addon id="repository.zukzeuk" name="ZukZeuk REPO" version="0.1" provider-name="ZukZeuk">
	<extension point="xbmc.addon.repository" name="STVA Repo">
		<info compressed="false">https://raw.githubusercontent.com/ezdakit/ZukZeuk_repo/main/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/ezdakit/ZukZeuk_repo/main/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/ezdakit/ZukZeuk_repo/main</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>ZukZeuk repo</summary>
   		<description lang="es">Repositorio de addons ZukZeuk</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

</addons>

